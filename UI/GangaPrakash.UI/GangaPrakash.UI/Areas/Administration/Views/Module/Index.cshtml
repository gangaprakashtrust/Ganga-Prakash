@using GangaPrakashAPI.Model
@model IEnumerable<Module>
<div class="wrapper wrapper-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox ">
                <div class="ibox-title">
                    <h5>Modules</h5>
                    <a class="btn btn-primary btn-rounded" href="@Url.Action("Create", "Module",new { Area="Administration"})" style="float:right;">Add</a>
                </div>
                <div class="ibox-content">

                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover dataTables-example">
                            <thead>
                                <tr>
                                    <th colspan="1">Sequence No.</th>
                                    <th colspan="1">Name</th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                <tr>
                                    <td colspan="1">@item.SequenceNo</td>
                                    <td colspan="1">@item.Name</td>
                                </tr>
                                }

                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>





