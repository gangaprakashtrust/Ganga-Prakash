@using GangaPrakashAPI.Model
@model IEnumerable<ApplicationUser>
<div class="wrapper wrapper-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox ">
                <div class="ibox-title">
                    <h5>ApplicationUsers</h5>
                    <a class="btn btn-primary btn-rounded" href="@Url.Action("Create", "ApplicationUser",new { Area="Administration"})" style="float:right;">Add</a>
                </div>
                <div class="ibox-content">
                    @if (TempData["Message"] != null)
                    {
                        <div class="@TempData["MessageClass"]">
                            <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
                            @TempData["Message"]
                        </div>
                    }
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover dataTables-example">
                            <thead>
                                <tr>
                                    <th colspan="1">UserName</th>
                                    <th colspan="1">Email</th>
                                    <th colspan="1">FirstName</th>
                                    <th colspan="1">LastName</th>
                                    <th colspan="1">MobileNo</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                <tr>
                                    <td colspan="1"><a href="/Administration/ApplicationUser/Edit?Id=@item.Id">@item.UserName</a></td>
                                    <td colspan="1">@item.Email</td>
                                    <td colspan="1">@item.FirstName</td>
                                    <td colspan="1">@item.LastName</td>
                                    <td colspan="1">@item.MobileNo</td>
                                </tr>
                                }

                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
@section scripts
{

    <script>
        $(document).ready(function () {
            $('.dataTables-example').DataTable({
                pageLength: 25,
                responsive: true,
                dom: '<"html5buttons"B>lTfgitp',
                buttons: [
                    { extend: 'copy' },
                    { extend: 'csv' },
                    { extend: 'excel', title: 'ApplicationUsers' },
                    { extend: 'pdf', title: 'ApplicationUsers' },

                    {
                        extend: 'print',
                        customize: function (win) {
                            $(win.document.body).addClass('white-bg');
                            $(win.document.body).css('font-size', '10px');

                            $(win.document.body).find('table')
                                .addClass('compact')
                                .css('font-size', 'inherit');
                        }
                    }
                ]

            });

        });

    </script>
}





